{
  "name": "tab-manager-v2",
  "version": "0.1.18",
  "description": "Quickly and conveniently manage your open tabs",
  "scripts": {
    "analyze": "NODE_ENV=production node utils/analyze.js",
    "build": "node utils/build.js",
    "deploy": "NODE_ENV=production yarn build && yarn zip",
    "zip": "zip -X --compression-method deflate -r build.zip build/*",
    "lint": "standard",
    "lintfix": "yarn lint -- --fix",
    "start": "node utils/webserver.js",
    "test": "yarn lint && yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "dependencies": {
    "chrome-extension-async": "^3.2.4",
    "dom-css": "^2.1.0",
    "fuzzy": "^0.1.3",
    "material-ui": "^1.0.0-beta.12",
    "material-ui-icons": "^1.0.0-beta.10",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2",
    "mousetrap": "^1.6.1",
    "react": "^16.0.0",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "^16.0.0",
    "react-flip-move": "^2.9.15",
    "rebound": "^0.0.15",
    "style-loader": "^0.18.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.11",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/react": "^3.2.11",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "file-loader": "1.1.4",
    "fs-extra": "^4.0.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "sinon-chrome": "^2.2.1",
    "standard": "^10.0.3",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.8.2",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "chrome"
    ]
  }
}
